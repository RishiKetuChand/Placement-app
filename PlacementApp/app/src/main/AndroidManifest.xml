<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.placementapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlacementApp">
        <activity android:name=".acitivties.UpdateAboutPlacementActivity"></activity>
        <activity
            android:name=".acitivties.StudentDetailsActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity
            android:name=".acitivties.StdentRegisterActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity
            android:name=".acitivties.RegisteredStudentsActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity
            android:name=".acitivties.PlacedStudentsActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity android:name=".acitivties.UploadStudentDetailsActivity" />
        <activity
            android:name=".acitivties.ViewCompanyActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity
            android:name=".acitivties.AddCompanyActivity"
            android:parentActivityName=".acitivties.ProfHomeActivity" />
        <activity android:name=".acitivties.ProfHomeActivity" />
        <activity android:name=".acitivties.OtpAuthActivity" />
        <activity android:name=".acitivties.LogInActivity" />
        <activity android:name=".acitivties.SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>